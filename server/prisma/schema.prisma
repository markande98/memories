datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

generator client {
  provider = "prisma-client-js"
}



model Photos{
  id String @id @default(uuid())
  userId String
  imageUrl String @db.Text

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
 }

model Videos{
  id String @id @default(uuid())
  userId String
  videoUrl String @db.Text

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}